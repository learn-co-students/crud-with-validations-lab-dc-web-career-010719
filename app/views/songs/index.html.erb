<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Song Title</th>
      <th>Released?</th>
      <th>Release Date</th>
      <th>Artist</th>
      <th>Genre</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= link_to s.title, s %> </td>
        <td><%= s.released %> </td>
        <td><%= s.release_year %> </td>
        <td><%= s.artist_name %> </td>
        <td><%= s.genre %> </td>
        <td><%= link_to "Delete", s, method: :delete, data: {confirm: "Really?"} %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to "Create A New Song", new_song_path %>
