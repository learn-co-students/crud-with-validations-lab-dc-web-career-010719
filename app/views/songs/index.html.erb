<h1>Songs#Index</h1>

<table style="width:100%">
  <tr>
    <th>Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>
  <%@songs.each do |song|%>
  <tr>
    <td><%=link_to song.title, song_path(song)%></td>
    <td><%=song.released%></td>
    <td><%=song.release_year%></td>
    <td><%=song.artist_name%></td>
    <td><%=song.genre%></td>
  </tr>
  <%end%>
</table>

<%=button_to "Create a new song", new_song_url, method: :get%>
