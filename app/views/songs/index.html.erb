<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
<% @songs.each do |s| %>
  <tr>
  <td><%= s.title %></td>
  <td><%= s.release_year %></td>
  <td><%= s.artist_name %></td>
  <td><%= s.genre %></td>
<%= link_to 'Delete', s, method: :delete, data: { confirm: 'Real?' } %>
</tr>
<% end %>
</tbody>
</table>
